ppt = ["espada", "pistola", "escudo"]
ppy = ppt[Math.floor(Math.random() * ppt.length)]
ppg = Math.floor(Math.random() * 1) + 10
pptb = ppy
if ((pptb == "espada" && args == "pistola") ||
(pptb == "pistola" && args == "escudo") ||
(pptb == "escudo" && args == "espada")) {
var vit = "vitoria"
} else if ((pptb == "espada" && args == "escudo") ||
(pptb == "pistola" && args == "espada") ||
(pptb == "escudo" && args == "pistola")) {
var vit = "derrota"
} else if ((pptb == "espada" && args == "espada") ||
(pptb == "pistola" && args == "pistola") ||
(pptb == "escudo" && args == "escudo")) {
var vit = "empate"
} else if (vit = "undefined") {
return reply(`Você deve digitar ${prefix}ppt espada, ${prefix}ppt pistola ou ${prefix}ppt escudo`)
}
if (vit == "vitoria") {
var tes = "Vitória do jogador"
}
if (vit == "derrota") {
var tes = "A vitória é do BOT"
}
if (vit == "empate") {
var tes = "O jogo terminou em empate"
}
